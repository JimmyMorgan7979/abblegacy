<div class="row mt-3">
    <div class="col-sm">
        <!--Spacing--> 
    </div>
    <div class="col-sm-6">
        <div class="text-left mb-4 h5">
            <ol class="list-unstyled">
                <% response.forEach(function(item){%>
                    <li class="my-3">
                        <form action="POST">
                            <strong>Date of Request: </strong><%= item.dateRequest %> </br>
                            <% if (item.partNumber) { %>
                                <strong class="pr-2">Board Part Number: </strong> <%= item.partNumber %><br>
                            <% } %>
                            <% if (item.serialNumber) { %>
                                <strong class="pr-2">Serial Number: </strong> <%= item.serialNumber %><br>
                            <% } %>
                            <% if (item.remanPrice) { %>
                                <strong class="pr-2">Reman Price: </strong> <%= item.remanPrice %><br>
                            <% } %>
                            <% if (item.exchangePrice) { %>
                                <strong class="pr-2">Exchange Price: </strong> <%= item.exchangePrice %><br>
                            <% } %>
                            <% if (item.stockedAS) { %>
                                <strong class="pr-2">Stocked AS: </strong> <%= item.stockedAS %><br>
                            <% } %>
                            <% if (item.quantity) { %>
                                <strong class="pr-2">Quantity: </strong> <%= item.quantity %><br>
                            <% } %>
                            <% if (item.description1) { %>
                                <strong class="pr-2">Description: </strong> <%= item.description1 %><br>
                            <% } %>
                            <% if (item.sapNumber) { %>
                                <strong class="pr-2">Sap Number: </strong> <%= item.sapNumber %><br>
                            <% } %>
                            <% if (item.price) { %>
                                <strong class="pr-2">Price: </strong> <%= item.price %><br>
                            <% } %>
                            <% if (item.name) { %>
                                <strong class="pr-2">Name: </strong> <%= item.name %><br>
                            <% } %>
                            <% if (item.company) { %>
                                <strong class="pr-2">Company: </strong> <%= item.company %><br>
                            <% } %>
                            <% if (item.email) { %>
                                <strong class="pr-2">Email: </strong> <%= item.email %><br>
                            <% } %>
                            <% if (item.phone) { %>
                                <strong class="pr-2">Phone: </strong> <%= item.phone %><br>
                            <% } %>
                            <% if (item.message) { %>
                                <strong class="pr-2">Message: </strong> <%= item.message %><br>
                            <% } %>
                            <a class="btn btn-danger btn-sm mt-2 text-white" id="deletebutton" href="/partRequest/requestDelete/<%=item._id%>/delete">Remove From List</a> 
                            <hr>
                        </form>
                    </li>
                <% }); %>
                <a class="btn btn-secondary btn-block mt-4 text-white" href="/partLogin/partAdmin">Go Back to Admin Page</a>
            </ol>
        </div>
    </div>
    <div class="col-sm">
        <!--Spacing--> 
    </div>
</div>